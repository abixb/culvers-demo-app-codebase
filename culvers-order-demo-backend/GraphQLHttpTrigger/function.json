{
    "bindings": [
      {
        "authLevel": "anonymous",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": [
          "get", 
          "post"
        ],
        "route": "graphql" 
      },
      {
        "type": "http",
        "direction": "out",
        "name": "res"
      }
    ],
    "scriptFile": "../dist/GraphQLHttpTrigger/index.js",
    "entryPoint": "graphqlHandler"
  }